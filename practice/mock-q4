
#Simple Function
#>>>
#The second number is smaller The first number is smaller Both numbers are equal
#>>>


def displaySmallest(num1, num2):
 
  if (num1 > num2):
    print ("The second number is smaller")
  elif(num1 < num2):
    print ("The first number is smaller")
  else:
    print("Both numbers are equal")

displaySmallest (7, 5)
displaySmallest (1, 3)
displaySmallest (4, 4)
